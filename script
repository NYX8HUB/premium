
local GI = game.PlaceId
local nix = (loadstring or load)

local fisch = "https://raw.githubusercontent.com/NYX8HUB/premium/refs/heads/main/games/fisch/script.lua"
local bloxf = "https://raw.githubusercontent.com/NYX8HUB/premium/refs/heads/main/games/blocfruits/script.lua"
local memesea = "https://raw.githubusercontent.com/NYX8HUB/premium/refs/heads/main/games/memesea/script.lua"

if GI == 16732694052 then
    nix(game:HttpGet(fisch))()
elseif GI == 7449423635 or GI == 4442272183 or GI == 2753915549 then
    nix(game:HttpGet(bloxf))()
elseif GI == 10260193230 then
    nix(game:HttpGet(memesea))()
else
    game.Players.LocalPlayer:Kick("Invalid game , send suggestions on the discord server : dsc.gg/nyx-hub")
end
